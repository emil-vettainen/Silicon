@{
    ViewData["Title"] = "Ooops! Page was not found";
}

<section class="not-found">
    <div class="container">
        <div class="content">
            <img id="not-found-img" src="/img/404/404.png" alt="Not Found">
            <h1 class="d5">Ooops!</h1>
            <p class="text-xl">The page you are looking for is not available.</p>
        </div>
        <nav>
            <a class="btn-theme-medium" asp-action="Home" asp-controller="Default"><i class="fa-regular fa-house"></i>Go to homepage</a>
        </nav>
    </div>
</section>